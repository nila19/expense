{
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "include": [
    "api/**/*.js"
  ],
  "exclude": [
    "test/**/*.spec.js",
    "api/bin/**/*.js",
    "api/config/route-config.js",
    "api/datamigrate/**/*.js",
    "api/utils/logger.js",
    "api/routes/**/*.js"
  ],
  "reporter": [
    "text",
    "lcov",
    "text-summary"
  ],
  "require": [],
  "extension": [],
  "cache": true,
  "all": true,
  "report-dir": "./coverage"
}
